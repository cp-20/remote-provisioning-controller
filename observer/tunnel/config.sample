Host *
	TCPKeepAlive yes
	ServerAliveInterval 60
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	User isucon
	LocalForward 0:3306 localhost:3306
	LocalForward 0:8888 localhost:8888
	LocalForward 0:9100 localhost:9100
	LocalForward 0:10008 localhost:10008

Host s1
	HostName ip-s1

Host s2
	HostName ip-s2

Host s3
	HostName ip-s3
